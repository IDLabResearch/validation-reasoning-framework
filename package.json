{
  "name": "n3unit",
  "version": "0.1.0",
  "description": "RDF Validation by reasoning, using N3 and EYE",
  "main": "index.js",
  "scripts": {
    "install": "node gather.js",
    "start": "cross-env DEBUG=N3Unit:server node ./bin/www",
    "start:dev": "cross-env NODE_ENV=development DEBUG=N3Unit:* ./node_modules/.bin/nodemon ./bin/www",
    "test": "mocha -R spec --delay",
    "forever": "cross-env DEBUG=N3Unit:server forever ./bin/www"
  },
  "keywords": [
    "RDF",
    "Validation",
    "N3",
    "EYE"
  ],
  "author": "bjdmeest",
  "license": "MIT",
  "devDependencies": {
    "chai": "^3.5.0",
    "eslint": "^3.19.0",
    "mocha": "^3.5.3",
    "nodemon": "^1.18.3"
  },
  "dependencies": {
    "async": "^2.6.1",
    "body-parser": "^1.18.3",
    "colors": "^1.3.1",
    "commander": "^2.17.1",
    "cookie-parser": "^1.4.3",
    "cross-env": "^5.2.0",
    "debug": "^3.1.0",
    "express": "^4.16.3",
    "forever": "^0.15.3",
    "fs-extra": "^2.1.2",
    "minimist": "^1.2.0",
    "morgan": "^1.9.0",
    "n3": "^0.10.0",
    "pug": "^2.0.3",
    "rdf-ext": "^0.3.0",
    "rdf-parser-rdfxml": "^0.3.1",
    "rdf-serializer-ntriples": "^0.3.0",
    "request": "^2.88.0",
    "shacl": "github:TopQuadrant/shacl-js",
    "tmp": "0.0.31"
  }
}
