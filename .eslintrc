{
  "rules": {
    "no-console": 0,
    "indent": [0, 2],
    "quotes": [
      0,
      "single"
    ],
    "semi": [
      2,
      "always"
    ]
  },
  "env": {
    "es6": true,
    "node": true
  },
  "ecmaFeatures": {
    "modules": true,
    "experimentalObjectRestSpread": true
  },
  "extends": "eslint:recommended"
}