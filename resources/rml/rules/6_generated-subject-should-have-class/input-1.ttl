@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix adms: <http://www.w3.org/ns/adms#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix transit: <http://vocab.org/transit/terms/> .

<#ProductIdentificatie>
    rml:subjectMap [
        rr:template "http://ewi.mmlab.be/cd/id/identificatie/beveren_{/GeefProductResponse/product/productId}";
        rr:class adms:Identifier
    ];
    rr:predicateObjectMap [
        rr:predicate dcterms:publisher;
        rr:objectMap [
          rr:template "http://ewi.mmlab.be/cd/id/identificatie/beveren_{/GeefProductResponse/product/productPublisher}";
        ]
    ].



